{
  "name": "ld-prototype-proxy",
  "version": "0.1",
  "description": "Authorization Proxy for SPARQL Endpoints",
  "main": "index.js",
  "type": "module",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "proxy-ewr": "node ./main.js ../config/ewr/ -p 3000",
    "proxy-upi": "node ./main.js ../config/upi/ -p 3001",
    "proxy-upi-admin": "node ./main.js ../config/upi/ -p 3001 -r admin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swiss/ld-prototype-proxy"
  },
  "keywords": [
    "sparql",
    "proxy",
    "authorization"
  ],
  "author": "Alain Rohrbach (alain.rohrbach@bfh.ch)",
  "license": "",
  "dependencies": {
    "@rdfjs/data-model": "^2.0.2",
    "body-parser": "^1.20.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "debug": "^4.3.5",
    "eslint-config-standard": "^17.1.0",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "express-ws": "^5.0.2",
    "jsonschema": "^1.4.1",
    "oxigraph": "^0.4.0-alpha.7",
    "sparql-http-client": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "globals": "^15.9.0"
  }
}
